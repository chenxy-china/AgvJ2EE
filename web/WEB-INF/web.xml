    </servlet>

    <servlet-mapping>
        <servlet-name>ApiHandlerSetPubMap</servlet-name>
        <url-pattern>/setpublishmap</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ApiHandlerRelocate</servlet-name>
