    <servlet-mapping>
        <servlet-name>ApiHandlerRelocateStop</servlet-name>
        <url-pattern>/stoprelocate</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ApiHandlerSetImuLocate</servlet-name>
