<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <!-- <listener> <listener-class>se.jiderhamn.classloader.leak.prevention.ClassLoaderLeakPreventorListener</listener-class> 
        </listener> -->

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>HelloServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.api.cxy.TestHandler</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ApiHandler</servlet-name>
        <servlet-class>com.api.cxy.ApiHandler</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>ApiHandler</servlet-name>
        <url-pattern>/agvinterface</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ApiHandlerCreMap</servlet-name>
        <servlet-class>com.api.cxy.ApiHandlerCreMap</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ApiHandlerCreMap</servlet-name>
        <url-pattern>/startcreatemap</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ApiHandlerCreMapStop</servlet-name>
        <servlet-class>com.api.cxy.ApiHandlerCreMapStop</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ApiHandlerCreMapStop</servlet-name>
        <url-pattern>/stopcreatemap</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ApiHandlerSaveMap</servlet-name>
        <servlet-class>com.api.cxy.ApiHandlerSaveMap</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ApiHandlerSaveMap</servlet-name>
        <url-pattern>/savemap</url-pattern>
    </servlet-mapping>

</web-app>